{
  "name": "z-spreadsheet",
  "version": "0.0.1",
  "description": "a javascript xpreadsheet",
  "types": "src/index.d.ts",
  "main": "src/index.js",
  "files": [
    "assets",
    "dist",
    "src"
  ],
  "author": "Pavel Rusakov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rskvp/z-spreadsheet.git"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/core/*.js",
      "src/locale/locale.js"
    ],
    "exclude": [
      "**/*.spec.js"
    ]
  },
  "scripts": {
    "dev": "webpack-dev-server --open --config build/webpack.dev.js",
    "build": "webpack --config build/webpack.prod.js",
    "build-locale": "webpack --config build/webpack.locale.js",
    "lint": "./node_modules/eslint/bin/eslint.js src",
    "test": "nyc ./node_modules/mocha/bin/mocha --require @babel/register --recursive test",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov -t 31ecdb12-8ecb-46f7-a486-65c2516307dd",
    "postinstall": "opencollective-postinstall"
  },
  "keywords": [
    "javascript",
    "spreadsheet",
    "canvas"
  ],
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.19.0",
    "@babel/register": "^7.18.9",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "codecov": "^3.8.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.23.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "sass": "^1.54.9",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "hot-formula-parser": "^4.0.0",
    "any-date-parser": "^1.5.4",
    "opencollective": "^1.0.3",
    "opencollective-postinstall": "^2.0.2"
  },
  "prettier": {
    "jsxBracketSameLine": true,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "es5",
    "proseWrap": "always"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/z-spreadsheet"
  }
}
